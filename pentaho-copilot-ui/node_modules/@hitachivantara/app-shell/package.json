{
  "name": "@hitachivantara/app-shell",
  "version": "0.36.0",
  "description": "AppShell Component",
  "author": "Hitachi Vantara - Boba Fett Team",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "module": "dist/esm/index.js",
  "typings": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:js": "npx vite build",
    "clean": "npx rimraf ./dist *.tgz tsconfig.tsbuildinfo",
    "dev": "npx vite build --watch",
    "lint-check": "npx eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "npm run lint-check -- --cache --fix",
    "lint:prettier": "npx prettier --write \"src/**/*.{html,css,js,jsx,ts,tsx}\"",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hitachivantara/app-shell-shared": "0.12.0",
    "@hitachivantara/uikit-react-core": "^5.27.5",
    "@hitachivantara/uikit-react-icons": "^5.6.6",
    "@mui/material": "^5.12.3",
    "i18next": "^22.0.8",
    "i18next-browser-languagedetector": "^7.0.1",
    "path-to-regexp": "^6.2.1",
    "react-error-boundary": "^4.0.10",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.0.0",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "gitHead": "57252f48d3b76927f7fc2c458fe4d77ffd6d0ca1"
}
